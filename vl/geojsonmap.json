{
  "$schema": "https://vega.github.io/schema/vega-lite/v2.1.json",
  "width": 1000,
  "height": 600,
  "data": {
    "url": "data/overijssel.geojson",
    "format": {
      "type": "json",
      "property": "features"
    },
    "as": "geo"
  },
  "projection": {
    "type": "mercator",
    "rotate": [0,0,0],
    "center": [100,100]
  },
  "mark": {
    "type": "geoshape",
    "stroke": "white",
    "fill": "#ccc"
  }
}
